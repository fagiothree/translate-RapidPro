[
  {
    "has_extraline": 0,
    "flow_id": "f959cd92-9e63-4477-8339-81cf32dda757",
    "bit_id": "00167667-1b33-40c6-ab6c-b50d7a16b2cb",
    "bit_type": "text",
    "type_id": 0,
    "text": "Hi! How are you feeling? ",
    "word_count": 0
  },
  {
    "flow_id": "f959cd92-9e63-4477-8339-81cf32dda757",
    "bit_id": "00167667-1b33-40c6-ab6c-b50d7a16b2cb",
    "bit_type": "quick_replies",
    "type_id": 0,
    "text": "Great",
    "note": "This is a quick reply and its translation should be uniquely identified by the corresponding argument",
    "word_count": 6
  },
  {
    "flow_id": "f959cd92-9e63-4477-8339-81cf32dda757",
    "bit_id": "00167667-1b33-40c6-ab6c-b50d7a16b2cb",
    "bit_type": "quick_replies",
    "type_id": 1,
    "text": "Okay",
    "note": "This is a quick reply and its translation should be uniquely identified by the corresponding argument",
    "word_count": 7
  },
  {
    "flow_id": "f959cd92-9e63-4477-8339-81cf32dda757",
    "bit_id": "00167667-1b33-40c6-ab6c-b50d7a16b2cb",
    "bit_type": "quick_replies",
    "type_id": 2,
    "text": "Bad",
    "note": "This is a quick reply and its translation should be uniquely identified by the corresponding argument",
    "word_count": 8
  },
  {
    "has_extraline": 0,
    "flow_id": "f959cd92-9e63-4477-8339-81cf32dda757",
    "bit_id": "d52796bd-ff4f-4ef2-8881-2d1844873943",
    "bit_type": "text",
    "type_id": 0,
    "text": "Sorry, I don't understand what you mean.",
    "word_count": 9
  },
  {
    "flow_id": "f959cd92-9e63-4477-8339-81cf32dda757",
    "bit_id": "fdab24ad-58d2-4021-b29e-8de87831e7df",
    "bit_type": "arguments",
